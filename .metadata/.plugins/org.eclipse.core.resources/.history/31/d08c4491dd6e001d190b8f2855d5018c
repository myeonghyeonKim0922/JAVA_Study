package quiz.Exception;

import java.io.IOException;

public class FileWriterEx {

	public static void main(String[] args) {
		FileWriter fw = null;
		try {
			fw = new FileWriter("file.txt");
			fw.write("Java");	
		}catch (IOException e) {
			e.printStackTrace();
		}finally {
			try { fw.close(); } catch
			(IOException e) {}
		}
	}

}
