package quiz.test;

public abstract class Quiz01 {
	class Solution {
	    public int solution(int n) {
	        int pizza = 7;
	        int result = 1;
	        for(int i=0; i < n; i++)
	            if(i / 7 == 0){
	                result += 1;
	            }
	        }
	    }
	}
	public static void main(String[] args) {
		int num = 10;
		if(num < 0) {
			System.out.println("음수");
		}else if(num > 0) {
			System.out.println("양수");			
		}else {
			System.out.println("0");						
		}
	}

}
