package quiz.test;

public abstract class Quiz01 {
	class Solution {
		int[] arr = { 1, 2, 3, 4, 5 };
		int[] reverseArr = new int[arr.length];
		  for (int i = arr.length - 1, j = 0; i >= 0; i--, j++) {
	            reverseArr[j] = arr[i];
	        }

	}
	public static void main(String[] args) {
		int num = 10;
		if(num < 0) {
			System.out.println("음수");
		}else if(num > 0) {
			System.out.println("양수");			
		}else {
			System.out.println("0");						
		}
	}

}
